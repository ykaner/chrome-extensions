{
  "manifest_version": 2,

  "name": "google fagots",
  "version": "1.0",

  "content_scripts": [
    {
      "all_frames": false,
      "js": [ "google fagots.js" ],
      "matches": [ "https://www.google.co.il/_/chrome/newtab*" ],
      "run_at": "document_end"
    }
  ],

    "permissions" :
    [
      "activeTab",
      "tabs"
    ]
}
